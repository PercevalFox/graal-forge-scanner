[settings]
# Webhook Discord pour alertes
discord_webhook_url = https://discord.com/api/webhooks/

# Fichier contenant la liste brute d’IPs (une par ligne)
mass_ips_file = single_target.txt

# Wordlist des endpoints sensibles
wordlist_file   = wordlist.txt

# Ports à scanner (séparés par des virgules)
ports           = 80,443,1337,3000,5000,8080,8888,9090

# Nombre de threads
threads         = 25

# Timeout en secondes pour chaque requête HTTP
timeout         = 5

# Dossier de sortie pour les logs mass scan
output_dir = output_mass
